<!DOCTYPE html>
<html>
<head>
  <title><%= source %></title>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta name="description" content="">
  <meta name="author" content="">
  <meta http-equiv="Content-Type" content="text/html;charset=utf-8">
  <link rel="shortcut icon" href="/images/tempage.png">
  <script src="<%= protocol %>://ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js"></script>
  <script src="<%= protocol %>://codemirror.net/lib/codemirror.js"></script>
  <link rel="stylesheet" href="<%= protocol %>://codemirror.net/lib/codemirror.css">
  <script src="<%= protocol %>://codemirror.net/addon/mode/loadmode.js"></script>
  <style type="text/css">  
     .CodeMirror {
        height: 100%;
    }
  </style>
</head>
<body>
  <script type="text/javascript">
$(function() {  
  CodeMirror.modeURL = "<%= protocol %>://codemirror.net/addon/mode/%N/%N.js";
  
  var myCodeMirror = CodeMirror(document.body, {
    lineNumbers: true    
  });
  
  $.get( "<%= source %>", function( data ) {
    myCodeMirror.setValue(data);
    myCodeMirror.setOption("mode", '<%= CodeMirrorMode %>');
    CodeMirror.autoLoadMode(myCodeMirror, '<%= CodeMirrorMode %>');    
  });
  
});    
  </script>
  <script>
    (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
    (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
    m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
    })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

    ga('create', 'UA-52739655-1', 'tempage.io');
    ga('send', 'pageview');
  </script>

</body>
</html>